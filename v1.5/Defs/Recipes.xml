<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!--9mm -->
    <RecipeDef Name="LTS_9mmBaseRecipe" Abstract="True">
        <description>Make bullets that go pew.</description>
        <jobString>Making bullets.</jobString>
        <workAmount>1200</workAmount>
        <workSkill>Crafting</workSkill>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Smith</effectWorking>
        <soundWorking>Recipe_Smith</soundWorking>
        <recipeUsers>
            <li>LTS_AmmunitionBench</li>
        </recipeUsers>
        <allowMixingIngredients>false</allowMixingIngredients>
        <skillRequirements>
            <Crafting>6</Crafting>
        </skillRequirements>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                        <li MayRequire="Argon.VMEu">VMEu_Lead</li>
                    </thingDefs>
                </filter>
                <count>30</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Steel</li>
                <li MayRequire="Argon.VMEu">VMEu_Lead</li>
            </thingDefs>
        </fixedIngredientFilter>
        <researchPrerequisite>Gunsmithing</researchPrerequisite>
    </RecipeDef>
    <RecipeDef ParentName="LTS_9mmBaseRecipe">
        <defName>Make_Ammo9mmFMJ</defName>
        <label>make 9mm FMJ ammo</label>
        <products>
            <LTS_9mmFMJ>100</LTS_9mmFMJ>
        </products>
        <researchPrerequisite>Machining</researchPrerequisite>
    </RecipeDef>
    <RecipeDef ParentName="LTS_9mmBaseRecipe">
        <defName>Make_Ammo9mmAP</defName>
        <label>make 9mm AP ammo</label>
        <products>
            <LTS_9mmAP>100</LTS_9mmAP>
        </products>
    </RecipeDef>
    <RecipeDef ParentName="LTS_9mmBaseRecipe">
        <defName>Make_Ammo9mmHP</defName>
        <label>make 9mm HP ammo</label>
        <products>
            <LTS_9mmHP>100</LTS_9mmHP>
        </products>
    </RecipeDef>
