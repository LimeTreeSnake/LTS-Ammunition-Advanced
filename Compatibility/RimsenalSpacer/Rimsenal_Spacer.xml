        </operations>
        <operations>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/TraderKindDef[defName = "Caravan_Spacer_BulkGoods"]/stockGenerators</xpath>
                <value>
                    <li Class="StockGenerator_Tag">
                        <tradeTag>AmmoIndustrial</tradeTag>
                        <thingDefCountRange>4~8</thingDefCountRange>
                        <countRange>100~250</countRange>
                    </li>
                    <li Class="StockGenerator_Tag">
                        <tradeTag>AmmoSpacer</tradeTag>
                        <thingDefCountRange>1~3</thingDefCountRange>
                        <countRange>50~150</countRange>
                    </li>
